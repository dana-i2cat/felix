<?xml version="1.0" encoding="UTF-8"?>
<rspec type="request"
       xmlns="http://www.geni.net/resources/rspec/3"
       xmlns:sharedvlan="http://www.geni.net/resources/rspec/ext/shared-vlan/1"
       xmlns:xs="http://www.w3.org/2001/XMLSchema-instance"
       xs:schemaLocation="http://www.geni.net/resources/rspec/3/request.xsd
            http://www.geni.net/resources/rspec/ext/shared-vlan/1/request.xsd">

    <node client_id="urn:publicid:tn-network1:"
          component_manager_id="rn:publicid:IDN+AIST+authority+tnrm"
          exclusive="false">
        <sliver_type name="nsi-aist-domain-edge-node-1"/>
        <interface client_id="urn:publicid:tn-network1:aist-stp1">
            <sharedvlan:link_shared_vlan name="urn:publicid:IDN+AIST+authority+tnrm+component+aist-stp1+vlan+1980-1989"
                                         vlantag="1980-1989"/>
        </interface>
        <interface client_id="urn:publicid:tn-network1:xxx-stp1"/>
        <interface client_id="urn:publicid:tn-network1:yyy-stp2"/>
        <interface client_id="urn:publicid:tn-network1:i2cat-stp2">
            <sharedvlan:link_shared_vlan name="urn:publicid:IDN+AIST+authority+tnrm+component+i2cat-stp2+vlan+1980-1989"
                                         vlantag="1980-1989"/>
        </interface>
    </node>

    <link client_id=" urn:publicid:IDN+AIST+authority+tnrm ">
        <component_manager name="urn:publicid:IDN+AIST+authority+tnrm"/>
        <interface_ref client_id="urn:publicid:tn-network1:aist-stp1"/>
        <interface_ref client_id="urn:publicid:tn-network1:xxx-stp1"/>
        <interface_ref client_id="urn:publicid:tn-network1:yyy-stp2"/>
        <interface_ref client_id="urn:publicid:tn-network1:i2cat-stp2"/>
        <property source_id="urn:publicid:eu-network1:zpoint"
                  dest_id="urn:publicid:jp-network1:apoint"
                  capacity="500"/>
    </link>
</rspec>
